#include<stdio.h>
int main()
{
     int t;
      scanf("%d",&t);
     int i=1;
     while (i<=t)
     {
      int a,b, avg;
       scanf("%d%d",&a,&b);
        avg=(a+b);
         if(avg%2==0)
           printf("%d",(a+b)/2);
         else
           printf("-1");
       i++;
     }
      
return 0;
}
